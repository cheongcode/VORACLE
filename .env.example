# VORACLE Environment Variables
# Copy this file to .env and fill in your values

# GRID API Configuration
# Get your API key from https://portal.grid.gg/
GRID_API_KEY=your_grid_api_key_here
GRID_API_URL=https://api-op.grid.gg/central-data/graphql

# Frontend URL (for CORS in production)
FRONTEND_URL=

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000

# Cache Configuration
CACHE_DIR=.cache
CACHE_TTL=3600

# Logging Configuration
LOG_LEVEL=INFO
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s

# Report Configuration
MAX_INSIGHTS=12
MIN_INSIGHTS=6
MAX_CONCURRENT_REQUESTS=5
